{
    "presets": [
        [
            "@babel/env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {
                    "@config": "./src/config",
                    "@routes": "./src/routes",
                    "@controllers": "./src/controllers",
                    "@models": "./src/models"
                },
                "extensions": ".js"
            }
        ]
    ]
}